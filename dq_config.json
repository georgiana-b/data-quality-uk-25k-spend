{
    "branch": "master",
    "cache_dir": "/home/g/Documents/uk_spend/fetched",
    "data_dir": "/home/g/Documents/uk_spend/data",
    "data_quality_spec": {
        "data_quality_spec_web": "https://raw.githubusercontent.com/frictionlessdata/data-quality-spec/master/spec.json"
    },
    "datapackage_file": "datapackage.json",
    "goodtables": {
        "goodtables_web": "http://goodtables.okfnlabs.org",
        "arguments": {
            "pipeline": {
                "processors": ["structure", "schema"],
                "options":{
                        "schema": {"case_insensitive_headers": true}
                },
                "encoding": "ISO-8859-2"
            },
            "batch": {
                "sleep":2,
                "data_key": "data",
                "format_key": "format",
                "schema_key": "schema"
            }
        }
    },
    "performance_file": "performance.csv",
    "publisher_file": "publishers.csv",
    "remotes": [
        "origin"
    ],
    "result_file": "results.csv",
    "run_file": "runs.csv",
    "source_file": "sources.csv",
    "assess_timeliness": true,
    "timeliness": {
        "extract_period": true,
        "timeliness_strategy": ["title", "data"],
        "max_empty_relevance_period": 60
    }
}
